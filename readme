#ipv4
sh cfddns.sh v4 token v4name#mydomain.com auto
#ipv6
sh cfddns.sh v6 token v6name#mydomain.com auto auto
#ipv4 and ipv6
sh cfddns.sh v4v6 token v46name#mydomain.com auto auto
#cname
sh cfddns.sh CNAME token cname#mydomain.com www.google.com
#txt
sh cfddns.sh TXT token ctxt#mydomain.com "Welcome to my txt record"
